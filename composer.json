{
  "name": "chrisandchris/symfony-rowmapper",
  "license": "GPL-2.0",
  "type": "project",
  "description": "A RowMapper and QueryBuilder, makes querying databases easier and better than orm",
  "keywords": [
    "rowmapper",
    "database",
    "orm",
    "symfony",
    "query",
    "querybuilder",
    "builder"
  ],
  "autoload": {
    "psr-0": {
      "ChrisAndChris\\Common\\RowMapper": "src/"
    }
  },
  "require": {
    "php": ">=5.6",
    "symfony/symfony": "2.3.*|2.7.*"
  },
  "suggests": {
    "phpseclib/phpseclib": "^2.0",
    "liuggio/fastest": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.1",
    "symfony/monolog-bundle": "^2.3",
    "sensio/distribution-bundle": "^2.3",
    "sensio/framework-extra-bundle": "^2.3",
    "sensio/generator-bundle": "^2.3",
    "incenteev/composer-parameter-handler": "^2.0",
    "matthiasnoback/symfony-config-test": "^1.1",
    "phpseclib/phpseclib": "^2.0"
  },
  "minimum-stability": "stable"
}
