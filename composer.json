{
  "name": "chrisandchris/passive-record-orm",
  "license": "GPL-2.0",
  "type": "project",
  "description": "A database layer, passive record and orm abstraction layer for your php application. ",
  "keywords": [
    "rowmapper",
    "database",
    "orm",
    "symfony",
    "query",
    "querybuilder",
    "builder",
    "mysql",
    "postgres",
    "pgsql"
  ],
  "autoload": {
    "psr-0": {
      "ChrisAndChris\\Common\\RowMapper": "src/"
    }
  },
  "require": {
    "php": ">=7",
    "symfony/symfony": "^2.7|^3.3"
  },
  "suggests": {
    "phpseclib/phpseclib": "^2.0",
    "liuggio/fastest": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7",
    "symfony/monolog-bundle": "^2.3",
    "sensio/distribution-bundle": "^2.3",
    "sensio/framework-extra-bundle": "^2.3",
    "sensio/generator-bundle": "^2.3",
    "incenteev/composer-parameter-handler": "^2.0",
    "matthiasnoback/symfony-config-test": "^1.1",
    "phpseclib/phpseclib": "^2.0"
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "v2.6.x-dev"
    }
  }
}
